{
    "num_devices": 1,
    "num_experiments_per_device": 3,
    "random_key": 42,
    "num_steps": 200000,
    "save_args": {
        "save_states_every": 25000,
        "save_train_stats_every": 1000,
        "save_test_stats_every": 1000,
        "save_grad_every": 25000,
        "save_hessian_every": -1
    },
    "model": {
        "model": "vgg11",
        "num_classes": 10,
        "activation_fn": "relu",
        "use_bn": false
    },
    "dataset": {
        "dataset": "cifar10",
        "batch_size": 64,
        "dataset_path": "./data/datasets/"
    },
    "optimizer": {
        "optimizer": "sgdm",
        "lr": 0.0128,
        "lambda_wd": 8e-05,
        "momentum": 0.9,
        "apply_wd_every": 1,
        "apply_wd_to": "conv&kernel|out&kernel"
    },
    "norm": {
        "change_scale": "identity",
        "norm_fn": "identity",
        "norm_multiply": 1,
        "norm_every": -1,
        "reverse_norms": false
    },
    "at_step": 200000,
    "save_path": "./data/sgdm_wobn/wd/8e-05/"
}