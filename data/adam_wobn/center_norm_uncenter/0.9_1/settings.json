{
    "num_devices": 1,
    "num_experiments_per_device": 3,
    "random_key": 42,
    "num_steps": 300000,
    "save_args": {
        "save_states_every": 300000,
        "save_train_stats_every": 1000,
        "save_test_stats_every": 1000,
        "save_grad_every": -1,
        "save_hessian_every": -1
    },
    "model": {
        "model": "vgg11",
        "num_classes": 10,
        "activation_fn": "relu",
        "use_bn": false
    },
    "dataset": {
        "dataset": "cifar10",
        "batch_size": 64,
        "dataset_path": "./data/datasets/"
    },
    "optimizer": {
        "optimizer": "adam",
        "lr": 0.0004,
        "lambda_wd": 0,
        "momentum": 0.9,
        "apply_wd_every": 1
    },
    "norm": {
        "change_scale": "identity",
        "norm_fn": "center_norm_uncenter",
        "norm_multiply": 0.9,
        "norm_every": 1,
        "reverse_norms": false,
        "apply_norm_to": "conv&kernel|out&kernel"
    },
    "at_step": 300000,
    "save_path": "./data/adam_wobn/center_norm_uncenter/0.9_1/"
}